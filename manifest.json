{
    "manifest_version": 3,
    "name": "AdSkipper",
    "version": "1.0",
    "description": "help to skip Youtube Ad",
    "action": {
        "default_icon": "pingu.png"
    },
    "content_scripts": [
        {
            "js": ["content.js"],
            "matches": [
                "https://www.youtube.com/*"
            ]
        }
    ],
    "icons": {
        "16": "pingu.png",
        "32": "pingu.png",
        "48": "pingu.png",
        "128": "pingu.png"
      }
  }